%.o: %.c
	if [ ! -f ./build/$@ ]; then $(CXX) $(CXXFLAGS) -c $(CFLAGS) $< -o ./build/$@; fi
%.o: %.cc
	if [ ! -f ./build/$@ ]; then $(CXX) $(CXXFLAGS) -c $(CFLAGS) $< -o ./build/$@; fi

